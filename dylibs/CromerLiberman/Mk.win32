# Make configuration for Win32 using MINGW and gfortran
F77        = gfortran -O2 -Wall
AR         = ar
RANLIB     = ranlib
CLLIB      = cldata.lib
SHAREDLIB  = cldata.dll
CC         = gcc -O2 
LINK       = gcc -O2 -shared
LINK_FLAGS = -Wl,--add-stdcall-alias -lgfortran -lgfortranbegin -lgcc -lm -mwindows


